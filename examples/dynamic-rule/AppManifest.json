            "datapoints": [
                {
                    "path": "Vehicle.Speed",
                    "required": "true",
                    "access": "read"
                },
                {
                    "path": "Vehicle.ADAS.ABS.IsEnabled",
                    "required": "true",
                    "access": "read"
                },
                {
                    "path": "Vehicle.ADAS.ABS.IsEngaged",
                    "required": "true",
                    "access": "read"
                }
            ]
        },
        "runtime": [
            "grpc://sdv.databroker.v1.Broker/Subscribe",
            "mqtt"
        ]
    }
]
